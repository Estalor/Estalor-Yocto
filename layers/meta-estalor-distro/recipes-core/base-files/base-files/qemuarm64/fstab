LABEL=kernel_a       /kernel_a            vfat       defaults,nofail,noatime  0  0
LABEL=kernel_b       /kernel_b            vfat       defaults,nofail,noatime  0  0
LABEL=data           /data                auto       defaults,nofail,noatime  0  2
LABEL=storage        /storage             auto       defaults,nofail,noatime  0  2